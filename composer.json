{
    "name": "tapp/laravel-ezeep",
    "description": "Ezeep client for Laravel applications",
    "keywords": [
        "tapp",
        "ezeep"
    ],
    "homepage": "https://github.com/TappNetwork/laravel-ezeep",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jacob Zlogar",
            "email": "jacob.zlogar@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Tapp\\Ezeep\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tapp\\Ezeep\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tapp\\Ezeep\\EzeepServiceProvider"
            ],
            "aliases": {
                "Ezeep": "Tapp\\Ezeep\\EzeepFacade"
            }
        }
    }
}
